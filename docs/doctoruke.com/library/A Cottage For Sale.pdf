<body>

    <main>
        <div class="playerBar">

            <a class="logo" href="/songs.html">
                <img class="responsive logoBig" src="_img/doctoruke_logo.png" alt="Doctor Uke" />
                <img class="responsive logoSmall" src="_img/doctoruke_logo_small.png" alt="Doctor Uke" />
            </a>

            <div class="internalPlayer">
                
                <h1><script>$('h1').prepend(songTitle)</script></h1>
                
                <a id="repeat" class="norepeat" href="javascript:void(0)">
                    <img class="responsive" src="_img/repeat_icon.png" title="Repeat" alt="Repeat" />
                </a>
                <script>
                $('#repeat').click(function(){
                    $(this).toggleClass('norepeat','yesrepeat');
                    if ( $(this).hasClass('norepeat') ) {
                        shouldRepeat = false;
                        $(this).css('opacity', '0.6');
                    } else {
                        shouldRepeat = true;
                        $(this).css('opacity', '1');
                    }
                });
                '<i class="fa fa-ban" aria-hidden="true"></i>'
                </script>

                <!-- Audio Player -->
                <audio class="mainPlayer" controls>
                    I'm sorry, your web browser does not support the Doctor Uke audio player.
                </audio>
                <script>
                $('audio.mainPlayer').prepend('<source src="/_player/_songs/' + audioFileNameMP3 + '" type="audio/mp3"><source src="/' + audioFileNameOGG + '" type="audio/ogg">');
                </script>
                <!-- END Audio Player -->
            </div>

        </div>

        <div class="embeddedPDF">            
            <script>
            // Print Out the PDF iFrames
            $('.embeddedPDF').prepend('<iframe id="nativePDF" src="' + pdfURLnative + '" width="100%" height="100%" allowfullscreen webkitallowfullscreen seamless="seamless"></iframe>');
            $('.embeddedPDF').prepend('<iframe id="tabletPDF" src="' + pdfURLtablet + '" width="100%" height="100%" allowfullscreen webkitallowfullscreen seamless="seamless"></iframe>');
            $('.embeddedPDF').prepend('<iframe id="mobilePDF" src="' + pdfURLmobile + '" width="100%" height="100%" style="border: none;"></iframe>');

            // Apply our styles to PDF viewer
            $('#tabletPDF').load(function(){
                $('#tabletPDF').contents().find('head').append('<link rel="stylesheet" href="/_player/_css/pdfstyles_min.css" />');
            });
            </script>
        </div>


