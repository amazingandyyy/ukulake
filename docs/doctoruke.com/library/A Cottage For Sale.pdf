    <script>
    // Set Up the Page with Song Details
    var audioFileNameMP3 = audioFileName + ".mp3";
    var audioFileNameOGG = audioFileName + ".ogg";
    var encodedPDFname = encodeURIComponent(pdfName);

    var getUrlParameter = function getUrlParameter(sParam) {
        var sPageURL = decodeURIComponent(window.location.search.substring(1)),
          sURLVariables = sPageURL.split('&'),
          sParameterName,
          i;

        for (i = 0; i < sURLVariables.length; i++) {
          sParameterName = sURLVariables[i].split('=');

          if (sParameterName[0] === sParam) {
            return sParameterName[1] === undefined ? true : sParameterName[1];
          }
        }
    };

    // Check if BAR parameter is set in URL and,
    // if so, show the baritone PDF instead and
    // make some SEO changes
    var showBaritone = getUrlParameter('bar');
    showBaritonePDFnow = false;


    function urlExists(url) {
        var http = new XMLHttpRequest();
        http.open('HEAD', url, false);
        http.send();
        if (http.status != 404) {
            showBaritonePDFnow = true;
        }
        else {
            showBaritonePDFnow = false;
        }
    }


    if (showBaritone === '1') {

        // Make sure baritone PDF exists
        var testBARurl = '/' + encodedPDFname + 'bar.pdf';
        urlExists(testBARurl);

        if (showBaritonePDFnow) {

            // Update PDF filaname to BAR
            encodedPDFname = encodedPDFname + 'bar';

            // Update the page title and meta
            var currentPageTitle = document.title;
            var updatedPageTitle = currentPageTitle.replace("Chords |", "Baritone Chords |");
            var updatedPageTitle = updatedPageTitle.replace(" | Audio", " (BAR) | Audio");
            document.title = updatedPageTitle;

            var currentMetaDescription = $('meta[name=description]').attr('content');
            var updatedMetaDescription = currentMetaDescription.replace("presented with ukulele chord diagrams", "presented with baritone ukulele chord diagrams");
            $('meta[name=description]').attr('content', updatedMetaDescription);

        } else {
            window.alert('Sorry! Baritone chords are not available for this song. Loading the standard chords.');
        }
        
    }

    var pdfURLtablet = "_viewerjs/?zoom=page-width#/" + encodedPDFname + ".pdf";
    var pdfURLnative = "/" + encodedPDFname + ".pdf#zoom=113";
    var pdfURLmobile = "https://docs.google.com/viewer?url=https%3A%2F%2Fwww.doctoruke.com%2F" + encodedPDFname + ".pdf&amp;embedded=true";

    // Set No Repeat by Default
    var shouldRepeat = false;
    var thePlayer;

    $(document).ready(function(){

        thePlayer = plyr.setup({
            tooltips: false,
            keyboardShortcuts: { global: true }
        });

        thePlayer[0].on('ended', function(event) {
            
            if (shouldRepeat) {
                thePlayer[0].restart();
                thePlayer[0].play();
            }

        });

    });
    </script>

